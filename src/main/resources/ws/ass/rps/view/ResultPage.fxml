<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="430.0" prefWidth="600.0" styleClass="anchor-pane" stylesheets="@../css/MorningTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ws.ass.rps.view.ResultPageController">
   <children>
      <Label fx:id="resultText" alignment="CENTER" contentDisplay="CENTER" layoutX="107.0" layoutY="111.0" prefHeight="100.0" prefWidth="386.0" textAlignment="CENTER">
         <font>
            <Font name="Segoe Print" size="26.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="152.0" layoutY="241.0" prefHeight="52.0" prefWidth="299.0" text="Do You Wanna Play Again ?">
         <font>
            <Font name="Segoe Print" size="16.0" />
         </font>
      </Label>
      <ButtonBar buttonMinWidth="80.0" layoutX="196.0" layoutY="293.0" prefHeight="25.0" prefWidth="176.0">
         <buttons>
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#restartGame" text="Play" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#endGame" text="Exit" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
